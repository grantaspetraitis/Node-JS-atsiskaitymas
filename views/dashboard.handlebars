{{#each result}}
<div class="container">
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <hr>
            <h6 class="card-subtitle mb-2 text-muted">{{this.email}}</h6>
            <hr>
            <h6 class="card-subtitle mb-2 text-muted">{{this.register_time}}</h6>
            <hr>
            <a href="#" class="card-link">Edit account details</a>
            <a href="#" class="card-link">Delete account</a>
        </div>
    </div>
</div>
{{/each}}