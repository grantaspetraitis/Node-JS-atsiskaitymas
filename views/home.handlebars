{{#each rows}}
<div class="d-inline-flex p-2">
    <div class="col-sm-6">
        <div class="card" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text">{{this.content}}</p>
                <a href="api/blog/{{this.id}}" class="btn btn-primary">Read</a>
            </div>
        </div>
    </div>
</div>
{{/each}}